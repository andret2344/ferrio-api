{
	"type": "project",
	"license": "proprietary",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.4",
		"doctrine/annotations": "^2.0.2",
		"doctrine/doctrine-bundle": "^2.16.1",
		"doctrine/doctrine-migrations-bundle": "^3.4.2",
		"doctrine/migrations": "^3.9.4",
		"doctrine/orm": "^3.5.2",
		"monolog/monolog": "^3.9.0",
		"nelmio/cors-bundle": "^2.5.0",
		"phpdocumentor/reflection-docblock": "^5.6.3",
		"phpstan/phpdoc-parser": "^2.3.0",
		"symfony/apache-pack": "^v1.0.1",
		"symfony/asset": "*",
		"symfony/console": "*",
		"symfony/dotenv": "*",
		"symfony/expression-language": "*",
		"symfony/flex": "^v2.8.2",
		"symfony/framework-bundle": "*",
		"symfony/property-access": "*",
		"symfony/property-info": "*",
		"symfony/proxy-manager-bridge": "v6.4.24",
		"symfony/runtime": "*",
		"symfony/security-bundle": "*",
		"symfony/serializer": "*",
		"symfony/twig-bundle": "*",
		"symfony/validator": "*",
		"symfony/webpack-encore-bundle": "^v2.3.0",
		"symfony/yaml": "*",
		"twig/twig": "^v3.21.1"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"allow-plugins": {
			"symfony/flex": true,
			"symfony/runtime": true
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php72": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "^7.3"
		}
	},
	"require-dev": {
		"dama/doctrine-test-bundle": "^8.3",
		"doctrine/doctrine-fixtures-bundle": "^4.1",
		"liip/test-fixtures-bundle": "^3.5",
		"phpunit/phpunit": "^12.3",
		"symfony/browser-kit": "*",
		"symfony/css-selector": "*",
		"symfony/http-client": "*",
		"symfony/maker-bundle": "^v1.64.0",
		"symfony/stopwatch": "*",
		"symfony/web-profiler-bundle": "*"
	}
}
